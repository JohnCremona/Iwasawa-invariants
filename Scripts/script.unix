#!/bin/bash
echo "Starting computation"

Nmin=1
Nmax=100

pmin=2
pmax=50

data='"data1.1000.2.50.txt"'
warn='"warn1.1000.2.50.txt"'
bcf='"bigcalls1-1000.txt"'
bc=30

Ncurrent=$Nmin

while [ $Ncurrent -lt $Nmax ]; do
	echo "Trying level $Ncurrent"
	sage -c "load('IwInv.sage'); print collect_iwasawa_invariants_of_ecs($Ncurrent,$Ncurrent,$pmin,$pmax,datafile=$data,\
warnfile=$warn,bigcallfile=$bcf,bigcall=$bc)"
	Ncurrent=$((Ncurrent+1))
done



       



